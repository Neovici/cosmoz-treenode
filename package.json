{
	"name": "@neovici/cosmoz-treenode",
	"version": "4.1.0",
	"description": "[![Build Status](https://travis-ci.org/Neovici/cosmoz-treenode.svg?branch=master)](https://travis-ci.org/Neovici/cosmoz-treenode)",
	"keywords": [
		"lit-element",
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-treenode#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-treenode/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-treenode.git"
	},
	"license": "Apache-2.0",
	"author": "Neovici dev",
	"main": "cosmoz-treenode.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"lint": "eslint --cache --ext .js .",
		"preversion": "npm run lint && npm run test -- --skip-plugin sauce",
		"postversion": "git push && git push --tags",
		"start": "polymer serve -o",
		"test": "karma start --coverage",
		"test:watch": "karma start --auto-watch=true --single-run=false",
		"test:update-snapshots": "karma start --update-snapshots",
		"test:prune-snapshots": "karma start --prune-snapshots",
		"test:compatibility": "karma start --compatibility all --auto-watch=true --single-run=false"
	},
	"dependencies": {
		"@neovici/computing-lit-element": "github:neovici/computing-lit-element#semver:^0.2.1",
		"@neovici/cosmoz-tree": "github:neovici/cosmoz-tree#semver:^3.0.1"
	},
	"devDependencies": {
		"@neovici/eslint-config": "github:neovici/eslint-config#semver:^1.0.0",
		"@neovici/github-actions-eslint-annotator": "^0.1.4",
		"@open-wc/testing": "^2.3.3",
		"@open-wc/testing-karma": "^3.1.33",
		"@polymer/iron-component-page": "^4.0.0",
		"@polymer/iron-demo-helpers": "^3.0.0",
		"deepmerge": "^4.0.0",
		"karma": "^4.3.0",
		"karma-firefox-launcher": "^1.2.0",
		"polymer-cli": "^1.9.7"
	}
}
