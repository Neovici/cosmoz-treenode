{
	"name": "@neovici/cosmoz-treenode",
	"version": "4.1.1",
	"description": "Utility class to manipulate a tree data structure.",
	"keywords": [
		"lit-element",
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-treenode#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-treenode/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-treenode.git"
	},
	"license": "Apache-2.0",
	"author": "Neovici dev",
	"main": "cosmoz-treenode.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"lint": "eslint --cache --ext .js .",
		"start": "polymer serve -o",
		"test": "karma start --coverage",
		"test:watch": "karma start --auto-watch=true --single-run=false",
		"test:update-snapshots": "karma start --update-snapshots",
		"test:prune-snapshots": "karma start --prune-snapshots",
		"test:compatibility": "karma start --compatibility all --auto-watch=true --single-run=false"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/github",
			"@semantic-release/npm",
			"@semantic-release/git"
		],
		"branch": "master"
	},
	"publishConfig": {
		"access": "public"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"dependencies": {
		"@neovici/computing-lit-element": "^0.4.0",
		"@neovici/cosmoz-tree": "^3.0.3"
	},
	"devDependencies": {
		"@commitlint/cli": "^8.2.0",
		"@commitlint/config-conventional": "^8.2.0",
		"@neovici/eslint-config": "^1.1.7",
		"@neovici/github-actions-eslint-annotator": "^0.3.0",
		"@open-wc/testing": "^2.3.5",
		"@open-wc/testing-karma": "^3.1.40",
		"@polymer/iron-component-page": "^4.0.0",
		"@polymer/iron-demo-helpers": "^3.0.0",
		"@semantic-release/changelog": "^3.0.4",
		"@semantic-release/git": "^7.0.16",
		"deepmerge": "^4.1.1",
		"husky": "^3.0.9",
		"karma": "^4.4.1",
		"karma-firefox-launcher": "^1.2.0",
		"karma-sauce-launcher": "^2.0.2",
		"polymer-cli": "^1.9.7",
		"semantic-release": "^16.0.0-beta.25"
	}
}
