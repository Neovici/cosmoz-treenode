{
	"name": "@neovici/cosmoz-treenode",
	"version": "5.1.0",
	"description": "Utility class to manipulate a tree data structure.",
	"keywords": [
		"lit-element",
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-treenode#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-treenode/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-treenode.git"
	},
	"license": "Apache-2.0",
	"author": "Neovici dev",
	"main": "cosmoz-treenode.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"lint": "eslint --cache --ext .js .",
		"start": "wds",
		"test": "wtr --coverage",
		"test:watch": "wtr --watch"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/github",
			"@semantic-release/npm",
			"@semantic-release/git"
		],
		"branch": "master"
	},
	"publishConfig": {
		"access": "public"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"dependencies": {
		"@neovici/cosmoz-tree": "^3.0.3",
		"haunted": "^5.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.0",
		"@commitlint/config-conventional": "^18.4.0",
		"@neovici/cfg": "^1.15.2",
		"@open-wc/testing": "^4.0.0",
		"@semantic-release/changelog": "^6.0.0",
		"@semantic-release/git": "^10.0.0",
		"@web/dev-server": "^0.4.0",
		"husky": "^8.0.3",
		"semantic-release": "^22.0.7"
	}
}
